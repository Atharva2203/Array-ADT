#include <iostream>
using namespace std;

void Set(int arr[],int index,int n,int x)
{
    if(index>=0 && index<n)
        arr[index]=x;
}

void Display(int arr[],int n)
{
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
}
int main()
{
    int index;
    int arr[]={2,3,15,20,40};
    int n=sizeof(arr)/sizeof(int);
    Set(arr,4,n,10);
    Display(arr,n);
    return 0;
}
