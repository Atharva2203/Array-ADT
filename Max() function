#include <iostream>
using namespace std;

int Max(int arr[],int n)
{
    int max=arr[0];
    for(int i=0;i<n;i++)
    {
        if(arr[i]>max)
            max=arr[i];
    }
    return max;
}

int main()
{
    int index;
    int arr[]={20,3,14,28,4};
    int n=sizeof(arr)/sizeof(int);
    cout<<Max(arr,n);
    return 0;
}
